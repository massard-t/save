package main

import "fmt"

func notation_avance(i int) {
	if (i >= 0 && i <= 5) {
		fmt.Print("Bouh ! C'est nul.\n");
	} else if (i >= 6 && i <= 9) {
		fmt.Print("C'est pas encore ça...\n");
	} else if (i == 10) {
		fmt.Print("C'était juste !\n");
	} else if (i >= 11 && i <= 15) {
		fmt.Print("Bien joué.\n");
	} else if (i >= 16 && i <= 19) {
		fmt.Print("Vous avez la maîtrise !\n");
	} else if (i == 20) {
		fmt.Print("C'est un perfect !\n");
	} else {
		fmt.Print("Va t'acheter des doigts !\n");
	}	
}

func main() {
	notation_avance(0);
	notation_avance(6);
	notation_avance(10);
	notation_avance(11);
	notation_avance(16);
	notation_avance(20);
	notation_avance(21);
}

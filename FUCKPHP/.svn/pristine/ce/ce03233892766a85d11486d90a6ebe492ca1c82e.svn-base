@font-face {
    font-family: raleway;
    src: url(../fonts/raleway.ttf);
	font-family: oswald;
    src: url(../fonts/Oswald-Light.ttf);
}

html
{
	background: url("../images/background.jpg") no-repeat center center fixed;
	-webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
    padding-top: 60px;
}
.hidden {
  display: none;
}

.count {
  box-shadow: 0 0 1px black;
}

.show {
  display: block;
}
body
{
	width: 1200px;
	margin: 20px auto;
	font-family: oswald;

}

a
{
	text-decoration: none;
	color: white;
	font-family: oswald;
}

.count
{
    display: inline-block;
    line-height: 1;
    vertical-align: baseline;
    margin: 0 .14285714em;
    background-color: #3ea9e8;
    background-image: none;
    padding: .1033em .523em;
    color: white;
    text-transform: none;
    font-weight: 700;
    border: 1px solid #3ea9e8;
    position: absolute;
    top: 10px;
    left: 1080px;
    border-radius: .28571429rem;
}

.no_float
{
  float: none !important;
}

.Shinra
{
  position:absolute;
  right:100px;
  bottom:10px;
}

.max_width
{
  width: 100%;
  text-align: center;
}

.wrapper
{
	margin: 20px auto;
	height: auto;
}

input, textarea {
	margin-right: 10%
  	margin: 0;
  	outline: 0;
  	width: 150px;
  	appearance: none;
  	margin: 20px;
   	line-height: 20px;
    padding: .67861429em 1em;
    font-size: 1em;
    background: #fff;
    border: 1px solid rgba(34,36,38,.15);
    color: rgba(0,0,0,.87);
    border-radius: .28571429rem;
    box-shadow: 0 0 0 0 transparent inset;
}

select {
	margin-right: 10%
  	margin: 0;
  	outline: 0;
  	width: 183px;
  	height: 43px;
  	appearance: none;
  	margin: 20px;
   	line-height: 20px;
    padding: .67861429em 1em;
    font-size: 1em;
    background: #fff;
    border: 1px solid rgba(34,36,38,.15);
    color: rgba(0,0,0,.87);
    border-radius: .28571429rem;
    box-shadow: 0 0 0 0 transparent inset;
}

select.short {
	margin-right: 10%
  	margin: 0;
  	outline: 0;
  	width: 71px;
  	display: inline;
  	height: 43px;
  	appearance: none;
  	margin: 20px;
   	line-height: 20px;
    padding: .57861429em 1em;
    font-size: 1em;
    background: #fff;
    border: 1px solid rgba(34,36,38,.15);
    color: rgba(0,0,0,.87);
    border-radius: .28571429rem;
    box-shadow: 0 0 0 0 transparent inset;
    font-size: 14px;
}

input:focus
{
	border: 1px solid lightblue;
	box-shadow: 0 0 5px lightblue;
}

select:focus
{
	border: 1px solid lightblue;
	box-shadow: 0 0 5px lightblue;
}

input[type=submit]:hover
{
	border: 1px solid lightblue;
	box-shadow: 0 0 5px lightblue;
}

input[type=reset]:hover
{
	border: 1px solid lightblue;
	box-shadow: 0 0 5px lightblue;
}

.image
{
	float: left;
	border-radius: 0.3125em;
}

.quick_action
{
	display: inline-block;
	margin: 20px;
  	background: transparent;
	float: left;
  	font-family: raleway;
  	padding: 13px;
  	font-weight: bold;
  	line-height: 13px;
  	text-align: center;
  	text-decoration: none;
  	border-radius: 5px;
  	background-color: transparent;
  	box-shadow: 0px 0px 0px 2px rgba(255, 255, 255, 0.5) inset;
  	color: #ffffff;
}
.quick_action img 
{
	filter: invert(100%);
    -webkit-filter: invert(100%);
    -moz-filter: invert(100%);
    -o-filter: invert(100%);
    -ms-filter: invert(100%);
}

h1, h3, h4
{
	text-transform: uppercase;
	font-family: raleway;
}

p
{
	font-family: raleway;
}

li
{
	font-family: raleway;
}

h2
{
	font-family: oswald !important;
}

.recent_products
{
	padding-bottom: 10px;
	padding-top: 5px;
	margin: 20px;
	overflow: hidden;
}

.quick_lauch
{
	float: right;
	margin-top: -10px;
}
.quick_action
{
  color: #ffffff;
}

.quick_action_a
{
  	display: inline-block;
	  margin: 20px;
  	background: transparent;
	  float: right;
  	font-family: raleway;
  	padding: 13px;
  	font-weight: bold;
  	line-height: 13px;
  	font-style: normal;
  	text-align: center;
  	text-decoration: none;
  	border-radius: 5px;
  	background-color: transparent;
  	box-shadow: 0px 0px 0px 2px rgba(255, 255, 255, 0.5) inset;
  	

  	-webkit-transition-property: background-color;
    -webkit-transition-duration: 2s, 2s;
    -moz-transition-property: background-color;
    -moz-transition-duration: 2s, 2s;
    transition-property: background-color;
    transition-duration: 2s, 2s;
}

.quick_action_b
{
    display: inline-block;
    margin: 20px;
    background: transparent;
    float: right;
    font-family: raleway;
    padding: 13px;
    font-weight: bold;
    line-height: 13px;
    font-style: normal;
    text-align: center;
    text-decoration: none;
    border-radius: 5px;
    background-color: transparent;
    box-shadow: 0px 0px 0px 2px black inset;
    color: black;

    

    -webkit-transition-property: background-color;
    -webkit-transition-duration: 2s, 2s;
    -moz-transition-property: background-color;
    -moz-transition-duration: 2s, 2s;
    transition-property: background-color;
    transition-duration: 2s, 2s;
}

.quick_action:hover
{
	color: grey;
	text-shadow: 0 0 10px black;
	box-shadow: 0 0 10px white;

	-webkit-transition-property: text-shadow, box-shadow, color;
    -webkit-transition-duration: 10s, 10s;
    -moz-transition-property: text-shadow, box-shadow, color;
    -moz-transition-duration: 10s, 10s;
    transition-property: text-shadow, box-shadow, color;
    transition-duration: 10s, 10s;

}

.quick_action_a:hover
{
  	background-color: #eee;
  	color: black;

    -webkit-transition-property: background-color, color;
    -webkit-transition-duration: 3s, 3s;
    -moz-transition-property: background-color, color;
    -moz-transition-duration: 3s, 3s;
    transition-property: background-color, color;
    transition-duration: 3s, 3s;
}

.quick_action_b:hover
{
    background-color: black;
    color: white;

    -webkit-transition-property: background-color, color;
    -webkit-transition-duration: 3s, 3s;
    -moz-transition-property: background-color, color;
    -moz-transition-duration: 3s, 3s;
    transition-property: background-color, color;
    transition-duration: 3s, 3s;
}

.right
{
	margin-left: 20px;
}


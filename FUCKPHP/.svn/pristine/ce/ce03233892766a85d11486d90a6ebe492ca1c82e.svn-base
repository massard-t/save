.nav
{
	list-style-type: none;
	display: inline;
}

.navbar li.logo
{

	margin-top : 7px;
	margin-right : 17px;
	color: white;
	display: inline;
	text-align: center;
	float: left;
	list-style: none;
}

.navbar li
{
	margin-top: 10px;
	margin-top: 20px;
	color: white;
	display: inline;
	text-align: center;
	float: left;
	list-style: none;
	font-family: oswald;
}

.logo
{
 	margin-top: 10px;
  

    -webkit-transition-property: invert, -webkit-filter, -moz-filter, -o-filter, -ms-filter;
    -webkit-transition-duration: 6s, 6s, 6s, 6s, 6s;
    -moz-transition-property:  invert, -webkit-filter, -moz-filter, -o-filter, -ms-filter;
    -moz-transition-duration: 6s, 6s, 6s, 6s, 6s;
    transition-property:  invert, -webkit-filter, -moz-filter, -o-filter, -ms-filter;
    transition-duration: 6s, 6s, 6s, 6s, 6s;
}

.logo:hover
{

	filter: invert(100%);
    -webkit-filter: invert(100%);
    -moz-filter: invert(100%);
    -o-filter: invert(100%);
    -ms-filter: invert(100%);

    -webkit-transition-property: invert, -webkit-filter, -moz-filter, -o-filter, -ms-filter;
    -webkit-transition-duration: 1s, 1s, 1s, 1s, 1s;
    -moz-transition-property:  invert, -webkit-filter, -moz-filter, -o-filter, -ms-filter;
    -moz-transition-duration: 1s, 1s, 1s, 1s, 1s;
    transition-property:  invert, -webkit-filter, -moz-filter, -o-filter, -ms-filter;
    transition-duration: 1s, 1s, 1s, 1s, 1s;
}

.navbar a
{
	color: whitesmoke;
	text-decoration: none;

	-webkit-transition-property: text-shadow, text-decoration, color;
    -webkit-transition-duration: 4s, 4s;
    -moz-transition-property: text-shadow, text-decoration, background-color, color;
    -moz-transition-duration: 4s, 4s;
    transition-property: text-shadow, text-decoration, background-color, color;
    transition-duration: 4s, 4s;
}

.navbar a:hover
{
	color: white;
	text-shadow: 0 0 10px black;
	text-decoration: bold;

	-webkit-transition-property: text-shadow, text-decoration, color;
    -webkit-transition-duration: 2s, 2s;
    -moz-transition-property: text-shadow, text-decoration, background-color, color;
    -moz-transition-duration: 2s, 2s;
    transition-property: text-shadow, text-decoration, background-color, color;
    transition-duration: 2s, 2s;
}

.separator
{
	margin-left: 10px;
	margin-right: 10px;

}

.core
{
	background-image: url("../images/quick_action_background.png");
    width: 98%;
    height: auto;
    padding: 10px;
    margin: auto;
    margin-bottom: 30px;
    border-radius: 5px;
    border: 1px solid grey;
    box-shadow: 0 0 3px black;
    margin-top: 0px;
    color: #ecf0f1;
    overflow: hidden;

    -webkit-transition-property: background-color;
    -webkit-transition-duration: 4s;
    -moz-transition-property: background-color;
    -moz-transition-duration: 4s;
    transition-property: background-color;
    transition-duration: 4s ;
}

.core:hover
{
	background-color: black;
	-webkit-transition-property: background-color;
    -webkit-transition-duration: 5s;
    -moz-transition-property: background-color;
    -moz-transition-duration: 8s;
    transition-property: background-color;
    transition-duration: 5s ;
}

.sidebar
{
	float: left;
	width: 30%;
	border-right: 1px dotted white;
	color: white;
}

.sidebar h3, .main h3
{
	color: whitesmoke;
	text-shadow: 0 0 2px black;
	text-transform: uppercase;
}

.clearfix::after {
  clear: both;
  display: block;
}

.main
{
	margin-left: 35%;
	margin-top: 34px;
	color: white;
	height: auto;
}

.contact td
{
	width: 100px;
}

footer
{
	position: fixed;
	padding-left: 60px;
	bottom: 0px;
	left: 0px;
	background-image: url("../images/stardust.png");
	color: white;
	width: 100%;
	text-shadow: 0 0 1px white;
	box-shadow: 0 0 5px black;
  opacity: 1.0;
}